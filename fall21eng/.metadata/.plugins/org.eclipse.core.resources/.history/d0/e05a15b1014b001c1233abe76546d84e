package day22Arrays;

import java.util.Arrays;

public class ArraysBinarysorch {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	String []isimler= {"hacer", "ayangul", "emine", "murat", "kutlu"};
	// isimler arrayinde murat ismi avr mý?
	
	Arrays.sort(isimler);
	System.out.println(Arrays.toString(isimler));
	
	
	// stringleri asci degelrine göre sýralar 
	
	isimler[4]="ahmet";
	Arrays.sort(isimler);
	System.out.println(Arrays.toString(isimler));
	
	System.out.println(Arrays.binarySearch(isimler, "murat"));
	
	
	
	
	
	
	
	
	
	}

}
