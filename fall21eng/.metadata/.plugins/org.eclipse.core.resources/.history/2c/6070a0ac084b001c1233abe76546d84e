package day22Arrays;

import java.util.Arrays;

public class SplitParçala {

	public static void main(String[] args) {
		// "java öðrenmek cok güzel "
// ters sýrada yazdýr 
		
		String cumle="java öðrenmek çok güzel";
		
		String arr[]=cumle.split(" ");
		System.out.println(Arrays.toString(arr));
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	}

}
