package _07_ForLoop;

public class Q02 {
    public static void main(String[] args) {


// 0-255 e kadar olan harflerin , sayÄı ve harf deÄŸerini konsola yazdÄ±rÄ±nÄ±z.

        
for (int i = 0; i < 255; i++) {
	char harf=(char)i; // data casting ile i variablı data casting komutuyla cahara atadık  variaballe buyuk oldugunda küçültmek için yapılır 
	System.out.println(i+" "+harf);

}
  
    
    
    
    
    
    
    
    
    }
}


