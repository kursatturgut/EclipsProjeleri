package day21;

import java.util.Arrays;

public class Arrayssorularýdevam {

	public static void main(String[] args) {
		// arrayýn tum elemanlarýný toplayýn
		
		int arr[]= {1,3,4,6,9,2};
		
		int toplam=0;
		
		for (int i = 0; i < arr.length; i++) {
			
	toplam+=arr[i];
		}
System.out.println("elementlerin toplemý:"+toplam);
		
	Arrays.sort(arr);	
		
	System.out.println(Arrays.toString(arr));	
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
